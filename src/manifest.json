{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.0.0",
  "description": "__MSG_extDesc__",
  "content_scripts": [
    {
      "matches": [
        "http://*.acfun.cn/v/*",
        "http://*.acfun.cn/bangumi/*",
        "http://*.hapame.com/video/*"
      ],
      "all_frames": true,
      "js": [
        "dom_gen.js",
        "resizeSensor.js",
        "rc4.js",
        "md5.js",
        "x2js.min.js",
        "flv.min.js",
        "hls.light.min.js",
        "hlsjsMediaInfo.min.js",
        "jquery-3.3.1.min.js",
        "google-style-loading.min.js",
        "CommentCoreLibrary.min.js",
        "biliplus_shield.min.js",
        "ABPlayer.min.js",
        "acfun_html5.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://v.youku.com/*"
      ],
      "all_frames": true,
      "js": [
        "dom_gen.js",
        "yk_ckey_getter.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://*/*",
    "storage",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "video_fix.js"
    ]
  },
  "icons": {
    "48": "icon.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    "ABPlayer.css"
  ],
  "minimum_chrome_version": "49.0"
}